{
    "blocksize": 1000000,
    "output": "output",
    "data": [
        {
            "path": "output/data.csv",
            "delimiter": ";",
            "encoding": "utf-8"
        }
    ],
    "tables": [
        {
            "name": "salary",
            "filters": [
                { "column": "salary", "type": "below", "value": 30001 }
            ],
            "columns": [ "year", "salary" ],
            "additions": [
                {
                    "name": "salaries per year", "type": "count",
                    "group": [ "year", "salary" ]
                }
            ],
            "rename columns": [
                { "column": "year", "new name": "yyyy" }
            ],
            "drop columns": [ "salary" ],
            "sorting": [
                {
                    "column": "yyyy",
                    "direction": "descending"
                }
            ]
        }
    ],
    "documents": [
        {
            "source": "input/document.md",
            "template": "input/template.odt",
            "language": "da-DK",
            "title": "Title",
            "author": "Firstname Lastname",
            "toc-title": "Contents"
        }
    ]
}