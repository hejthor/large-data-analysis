{
    "memory": "2GB",
    "output": "output",
    "data": [
        { "path": "output/data.csv", "delimiter": ";", "encoding": "utf-8" }
    ],
    "documents": [
        {
            "source": "input/document.md",
            "template": "input/template.odt",
            "language": "da-DK",
            "title": "Title",
            "author": "Firstname Lastname",
            "toc-title": "Contents"
        }
    ],
    "tables": [
        {
            "data": "output/parquets/data",
            "name": "users per month",
            "columns": [
                {
                    "name": "year (yyyy)",
                    "column": "date",
                    "type": "split",
                    "on": "-",
                    "select": 0,
                    "sorting": "descending",
                    "filters": [ { "type": "above", "value": 2020 } ]
                },
                {
                    "name": "month (mm)",
                    "column": "date",
                    "type": "split",
                    "on": "-",
                    "select": 1,
                    "sorting": "descending",
                    "replacements": {
                        "01": "January", "02": "February", "03": "March",
                        "04": "April", "05": "May", "06": "June",
                        "07": "July", "08": "August", "09": "September",
                        "10": "October", "11": "November", "12": "December"
                    }
                },
                {
                    "name": "user count",
                    "column": "username",
                    "group": [ "year (yyyy)", "month (mm)" ],
                    "type": "count"
                },
                {
                    "name": "unique users",
                    "column": "username",
                    "group": [ "year (yyyy)", "month (mm)" ],
                    "type": "unique count"
                }
            ]
        }
    ]
}